<%= content_for :title, "Actualité" %>
<div class="main">
  <div class="section text-center landing-section">
    <div class="container">
      <div class="row">
        <div class="col-md-8 col-md-offset-2">
          <h2>Actualités</h2>
          <h5>Ici vous trouverez les diverses publications et informations pouvant etre rendu
            publiques dans ce
            site.</h5>
          <br />
          <a href="actualite.html" class="btn btn-da btn-fill">Actualités</a>
        </div>
      </div>
    </div>
  </div>
  <div class="section  landing-section">
    <div class="explore">

      <div class="explores">

        <% if !@explore.empty? %>
        <% @explore.each do |e| %>
        <%= render 'partials/list', type: 'explores', f: e %>
        <% end %>
        <% end %>

        <% if @explore.empty? %>
        <%= render 'partials/nothing', mssg: "No one to explore!!" %>
        <% end %>

        <% if !@explore.empty? %>
        <%= render 'partials/end' %>
        <% end %>

      </div>

    </div>
  </div>
</div>